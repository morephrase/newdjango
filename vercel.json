{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "env": {
    "DEBUG": "@debug",
    "SECRET_KEY": "@secret_key",
    "DATABASE_URL": "@database_url",
    "ALLOWED_HOSTS": "@allowed_hosts"
  }
}
